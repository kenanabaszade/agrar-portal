        "search": "Axtarış sözü",
        "per_page": "Səhifə başına element sayı"
      }
    },
    "get_featured_programs": {
      "method": "GET",
      "url": "/api/v1/internship-programs/featured",
      "description": "Seçilmiş staj proqramlarını gətirir"
    },
    "get_categories": {
      "method": "GET",
      "url": "/api/v1/internship-programs/categories",
      "description": "Mövcud kateqoriyaları gətirir"
    },
    "get_program_details": {
      "method": "GET",
      "url": "/api/v1/internship-programs/{id}",
      "description": "Müəyyən staj proqramının ətraflı məlumatlarını gətirir"
    },
    "create_program": {
      "method": "POST",
      "url": "/api/v1/internship-programs",
      "description": "Yeni staj proqramı yaradır (admin/trainer)",
      "headers": {
        "Authorization": "Bearer {token}",
        "Content-Type": "application/json"
      }
    },
    "update_program": {
      "method": "PUT/PATCH",
      "url": "/api/v1/internship-programs/{id}",
      "description": "Staj proqramını yeniləyir (admin/trainer)",
      "headers": {
        "Authorization": "Bearer {token}",
        "Content-Type": "application/json"
      }
    },
    "delete_program": {
      "method": "DELETE",
      "url": "/api/v1/internship-programs/{id}",
      "description": "Staj proqramını silir (admin/trainer)",
      "headers": {
        "Authorization": "Bearer {token}"
      }
    }
  },
